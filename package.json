    "@angular/router": "3.0.0",
    "angular2-datatable": "0.4.2",
    "angular2-materialize": "3.0.3",
    "chart.js": "2.2.2",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "highlight.js": "9.6.0",
